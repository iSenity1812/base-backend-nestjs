{
  "name": "skeleton-backend-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "format:quick": "pretty-quick --pattern \"src/**/*.ts\" --pattern \"test/**/*.ts\" --pattern \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "nest start --watch",
    "dev:reset": "typeorm schema:drop -d db/migration.config.ts && typeorm migration:run -d db/migration.config.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration": "node ./tools/migration-cli.js",
    "prepare": "husky",
    "plop": "plop --plopfile plopfile.js",
    "generate:entity-package": "plop --plopfile plopfile.js entity-package",
    "generate:command": "plop --plopfile plopfile.js command",
    "release": "standard-version"
  },
  "dependencies": {
    "@langchain/core": "^0.3.75",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/openai": "^0.6.11",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.8",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.12",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "ioredis": "^5.7.0",
    "lodash": "^4.17.21",
    "nestjs-cls": "^6.0.1",
    "openid-client": "^5.2.0",
    "pg": "^8.13.1",
    "pixl-cache": "^1.0.6",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@clack/prompts": "^0.11.0",
    "@eslint/js": "^9.33.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/config": "^3.3.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lint-staged": "^15.4.3",
    "plop": "^4.0.1",
    "prettier": "^3.0.0",
    "pretty-quick": "^4.2.2",
    "source-map-support": "^0.5.21",
    "standard-version": "^9.5.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --fix",
    "*.{json,md,yml,yaml}": "prettier --write"
  }
}
